# CraftX OAuth Environment Variables
# Copy this file to .env and fill in your OAuth provider credentials

# Google OAuth
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# GitHub OAuth  
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Microsoft OAuth
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here

# Okta OAuth
OKTA_CLIENT_SECRET=your_okta_client_secret_here

# ORCID OAuth
ORCID_CLIENT_SECRET=your_orcid_client_secret_here

# Apple OAuth
APPLE_CLIENT_SECRET=your_apple_client_secret_here

# Auth0 OAuth
AUTH0_CLIENT_SECRET=your_auth0_client_secret_here

# Discord OAuth
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# LinkedIn OAuth
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret_here

# Twitter/X OAuth
TWITTER_CLIENT_SECRET=your_twitter_client_secret_here

# Additional settings
SESSION_SECRET_KEY=your_session_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here
