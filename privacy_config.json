{
  "privacy_settings": {
    "microsoft_telemetry_disabled": true,
    "azure_insights_disabled": true,
    "custom_analytics_enabled": true,
    "data_retention_days": 30,
    "local_storage_only": true
  },
  "custom_analytics": {
    "enabled": true,
    "storage_location": "local_sqlite",
    "collect_errors": true,
    "collect_usage": true,
    "collect_personal_data": false
  },
  "telemetry_opt_out": {
    "microsoft": true,
    "azure": true,
    "third_party": true
  }
}